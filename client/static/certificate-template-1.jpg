<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#f8f9fa" />
  <rect x="30" y="30" width="740" height="540" rx="5" fill="#ffffff" stroke="#C9B037" stroke-width="3" />
  <rect x="50" y="50" width="700" height="500" rx="5" fill="#ffffff" stroke="#C9B037" stroke-width="1" />
  
  <!-- Certificate title -->
  <text x="400" y="120" font-family="Arial" font-size="36" text-anchor="middle" fill="#000000">شهادة شكر وتقدير</text>
  
  <!-- Decorative line -->
  <path d="M300 140 L500 140" stroke="#C9B037" stroke-width="2" />
  
  <!-- Recipient name -->
  <text x="400" y="220" font-family="Arial" font-size="24" text-anchor="middle" fill="#000000">تمنح إلى</text>
  <text x="400" y="260" font-family="Arial" font-size="28" text-anchor="middle" fill="#000000">الاسم</text>
  
  <!-- Certificate text -->
  <text x="400" y="320" font-family="Arial" font-size="16" text-anchor="middle" fill="#000000">وذلك نظير جهوده المتميزة في</text>
  <text x="400" y="350" font-family="Arial" font-size="16" text-anchor="middle" fill="#000000">تفعيل أنشطة المدرسة</text>
  <text x="400" y="380" font-family="Arial" font-size="16" text-anchor="middle" fill="#000000">مع تمنياتنا له بدوام التوفيق</text>
  
  <!-- Signature placeholders -->
  <text x="200" y="480" font-family="Arial" font-size="14" text-anchor="middle" fill="#000000">المدير</text>
  <path d="M150 450 L250 450" stroke="#000000" stroke-width="1" />
  
  <text x="400" y="480" font-family="Arial" font-size="14" text-anchor="middle" fill="#000000">المشرف</text>
  <path d="M350 450 L450 450" stroke="#000000" stroke-width="1" />
  
  <text x="600" y="480" font-family="Arial" font-size="14" text-anchor="middle" fill="#000000">رائد النشاط</text>
  <path d="M550 450 L650 450" stroke="#000000" stroke-width="1" />
  
  <!-- Logo placeholders -->
  <circle cx="100" cy="100" r="40" fill="#ffffff" stroke="#000000" stroke-width="1" />
  <circle cx="400" cy="100" r="20" fill="#ffffff" stroke="#000000" stroke-width="1" />
  <circle cx="700" cy="100" r="40" fill="#ffffff" stroke="#000000" stroke-width="1" />
  
  <!-- Decorative corners -->
  <path d="M60 60 L100 60 L100 100" fill="none" stroke="#C9B037" stroke-width="2" />
  <path d="M700 60 L740 60 L740 100" fill="none" stroke="#C9B037" stroke-width="2" />
  <path d="M60 540 L100 540 L100 500" fill="none" stroke="#C9B037" stroke-width="2" />
  <path d="M700 540 L740 540 L740 500" fill="none" stroke="#C9B037" stroke-width="2" />
</svg>